{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "./*.ts",
    "routes/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}